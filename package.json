{
  "name": "application",
  "version": "0.1.0",
  "description": "Base NestJs application with JWT authentication.",
  "author": "Harivansh",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "lint": "eslint --cache --fix .",
    "format": "prettier --write . --cache-location ./.prettiercache",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "yarn node dist/main",
    "test:unit": "jest",
    "test:cli:unit": "jest --config ./cli/jest.config.ts",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "yarn node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest.config.ts --runInBand",
    "key:generate": "yarn run ts-node ./cli/script/generate-keys.script.ts --length 64 --env-file .env --env-vars JWT_ACCESS_TOKEN_SECRET JWT_REFRESH_TOKEN_SECRET",
    "db:seed": "yarn run ts-node --files --require tsconfig-paths/register ./cli/script/seed.script.ts",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/mongoose": "^10.0.1",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/terminus": "^10.1.1",
    "@nestjs/throttler": "^5.0.1",
    "argon2": "^0.31.1",
    "axios": "^1.6.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "helmet": "^7.0.0",
    "mongodb": "^6.1.0",
    "mongoose": "^7.6.2",
    "ms": "^2.1.3",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.2.0",
    "@jest/test-sequencer": "^29.7.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.6",
    "@types/ms": "^0.7.33",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^3.0.10",
    "@types/passport-local": "^1.0.36",
    "@types/supertest": "^2.0.12",
    "@types/yargs": "^17.0.29",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "chalk": "^4",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "express-list-routes": "^1.1.9",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.0.1",
    "mongodb-memory-server": "^9.0.1",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "yargs": "^17.7.2"
  },
  "packageManager": "yarn@3.6.4"
}
