{
  "name": "nestradamus",
  "version": "0.1.2",
  "description": "An opinionated starter project.",
  "author": "Harivansh",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "lint": "eslint",
    "format": "prettier --cache --cache-location ./.prettiercache --write .",
    "cli:client": "nest start --path ./apps/client/tsconfig.cli.json --entryFile ./apps/client/cli/main.js --",
    "repl:client": "nest start --watch --path ./apps/client/tsconfig.app.json --entryFile ./apps/client/src/repl.js --",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:dev:debug": "nest start --debug --watch",
    "prod:client": "node dist/client/apps/client/src/main.js",
    "prod:administration": "node dist/administration/main.js",
    "test:unit": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./jest.e2e-config.ts --runInBand",
    "postinstall": "husky"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.705.0",
    "@aws-sdk/s3-request-presigner": "^3.705.0",
    "@nestjs/common": "^10.4.15",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.15",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^10.4.15",
    "@nestjs/terminus": "^10.2.3",
    "argon2": "^0.41.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "helmet": "^8.0.0",
    "html-minifier": "^4.0.0",
    "html-to-text": "^9.0.5",
    "mjml": "^4.15.3",
    "mongodb": "^6.11.0",
    "mustache": "^4.2.0",
    "nodemailer": "^6.9.16",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "uuid": "^11.0.3",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@eslint/markdown": "^6.2.1",
    "@faker-js/faker": "^9.3.0",
    "@jest/test-sequencer": "^29.7.0",
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.15",
    "@trivago/prettier-plugin-sort-imports": "^5.2.0",
    "@types/cookie-parser": "^1.4.8",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^5.0.0",
    "@types/html-minifier": "^4.0.5",
    "@types/html-to-text": "^9.0.4",
    "@types/jest": "^29.5.14",
    "@types/jsdom": "^21.1.7",
    "@types/mjml": "^4.7.4",
    "@types/multer": "^1.4.12",
    "@types/mustache": "^4.2.5",
    "@types/node": "^22.10.1",
    "@types/nodemailer": "^6.4.17",
    "@types/supertest": "^6.0.2",
    "axios": "^1.7.9",
    "eslint": "^9.16.0",
    "globals": "^15.13.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.10",
    "mongodb-memory-server": "^10.1.2",
    "nest-commander": "^3.15.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.0"
  },
  "packageManager": "yarn@4.5.3"
}
