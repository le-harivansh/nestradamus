{
  "name": "application",
  "version": "0.1.0",
  "description": "Base NestJs application with JWT authentication.",
  "author": "Harivansh",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "lint": "eslint --cache --fix .",
    "format": "prettier --write . --cache-location ./.prettiercache",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "yarn node dist/main",
    "test:unit": "jest",
    "test:cli:unit": "jest --config ./cli/jest.config.ts",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "yarn node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest.config.ts --runInBand",
    "test:all": "yarn run test:unit && yarn run test:cli:unit && yarn run test:e2e",
    "key:generate": "yarn run ts-node ./cli/script/generate-keys/script.ts --length 64 --env-file .env --env-vars USER_JWT_ACCESS_TOKEN_SECRET USER_JWT_REFRESH_TOKEN_SECRET ADMINISTRATOR_JWT_ACCESS_TOKEN_SECRET ADMINISTRATOR_JWT_REFRESH_TOKEN_SECRET",
    "db:seed": "yarn run ts-node --files --require tsconfig-paths/register ./cli/script/seeder/script.ts",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.1",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/event-emitter": "^2.0.3",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.4",
    "@nestjs/mongoose": "^10.0.2",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/terminus": "^10.2.0",
    "@nestjs/throttler": "^5.1.1",
    "argon2": "^0.31.2",
    "axios": "^1.6.2",
    "bull": "^4.12.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "html-to-text": "^9.0.5",
    "mjml": "^4.14.1",
    "mongodb": "^6.3.0",
    "mongoose": "^8.0.3",
    "ms": "^2.1.3",
    "nodemailer": "^6.9.7",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@jest/test-sequencer": "^29.7.0",
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.3.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/express": "^4.17.21",
    "@types/html-to-text": "^9.0.4",
    "@types/jest": "^29.5.11",
    "@types/mjml": "^4.7.4",
    "@types/ms": "^0.7.34",
    "@types/node": "^20.10.5",
    "@types/nodemailer": "^6.4.14",
    "@types/supertest": "^6.0.1",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "chalk": "^4.1.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "express-list-routes": "^1.1.9",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "mailhog": "^4.16.0",
    "mongodb-memory-server": "^9.1.3",
    "prettier": "^3.1.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "yargs": "^17.7.2"
  },
  "packageManager": "yarn@4.0.2"
}
